package controle_atendimento_de_pacientes;

public class Executor {

	public static void main(String[] args) {
		BaseDados db = new BaseDados();
		System.out.println("Lista de especialidades:");
		for (int i = 0; i < db.especialidades().size(); i++) {
			System.out.println(db.especialidades().get(i).getNome());
		}

	}

}
